    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependenciesMeta": {
    "@vitest/coverage-v8": {
      "optional": false
    },
    "@vitest/ui": {
      "optional": false
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
