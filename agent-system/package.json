{
  "name": "corev4-agent-system",
  "version": "1.0.0",
  "description": "Multi-Agent Development System for CoreV4 Mental Health Platform",
  "main": "orchestrator.ts",
  "scripts": {
    "start": "tsx launch.ts",
    "launch:full": "tsx launch.ts --full",
    "launch:quick": "tsx launch.ts --quick",
    "status": "tsx launch.ts --status",
    "report": "tsx launch.ts --report",
    "test:agents": "vitest run --dir teams",
    "dev": "tsx watch launch.ts",
    "build": "tsc",
    "clean": "rm -rf dist reports logs"
  },
  "keywords": [
    "multi-agent",
    "ai",
    "development",
    "automation",
    "mental-health",
    "corev4"
  ],
  "author": "CoreV4 Development Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "figlet": "^1.7.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "events": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}